{
  "name": "adrianduffell/etsy",
  "description": "WordPress widget to display the latest products from a shop as a grid of thumbnails.",
  "license": "GPL-3.0-or-later",
  "require-dev": {
    "phpunit/phpunit": "^6.5",
    "dealerdirect/phpcodesniffer-composer-installer": "^0.6.2",
    "automattic/phpcs-neutron-standard": "~1.5",
    "wp-coding-standards/wpcs": "~2.2"
  },
  "require": {
    "pimple/pimple": "~3.0"
  },
  "autoload": {
    "classmap": ["src/"]
  },
  "scripts": {
    "phpunit" : "./vendor/bin/phpunit",
    "phpcs" : "./vendor/bin/phpcs -s",
    "test": [
      "@phpunit",
      "@phpcs"
    ]
  }
}
